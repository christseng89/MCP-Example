    <bpmn:dataObject id="DO_RejectNotice" name="拒绝通知" />
    <bpmn:startEvent id="Start_Submit" name="客户提交贷款申请" />
    <bpmn:task id="Task_Precheck" name="初审">
      <bpmn:dataOutputAssociation id="DataOutputAssociation_1ndzmwq">
        <bpmn:targetRef>DOR_Precheck</bpmn:targetRef>
      </bpmn:dataOutputAssociation>
      <bpmn:dataOutputAssociation id="DataOutputAssociation_0k33mk4">
        <bpmn:targetRef>DOR_Application</bpmn:targetRef>
      </bpmn:dataOutputAssociation>
    </bpmn:task>
    <bpmn:task id="Task_RiskAssess" name="风险评估">
      <bpmn:dataOutputAssociation id="DataOutputAssociation_1du3hdg">
        <bpmn:targetRef>DOR_Risk</bpmn:targetRef>
      </bpmn:dataOutputAssociation>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_Pass" name="是否通过？" />
    <bpmn:task id="Task_GenerateContract" name="合同生成（系统）">
      <bpmn:dataOutputAssociation id="DataOutputAssociation_012dmbp">
        <bpmn:targetRef>DOR_Contract</bpmn:targetRef>
      </bpmn:dataOutputAssociation>
      <bpmn:dataOutputAssociation id="DataOutputAssociation_0qirhrg">
        <bpmn:targetRef>DataStoreReference_084wzik</bpmn:targetRef>
      </bpmn:dataOutputAssociation>
    </bpmn:task>
    <bpmn:task id="Task_Disburse" name="放款">
      <bpmn:dataOutputAssociation id="DataOutputAssociation_12fwdhk">
        <bpmn:targetRef>DOR_Voucher</bpmn:targetRef>
      </bpmn:dataOutputAssociation>
      <bpmn:dataOutputAssociation id="DataOutputAssociation_0avnghi">
        <bpmn:targetRef>DataStoreReference_1ei56eu</bpmn:targetRef>
      </bpmn:dataOutputAssociation>
    </bpmn:task>
    <bpmn:task id="Task_NotifyReject" name="发送拒绝通知">
      <bpmn:dataOutputAssociation id="DataOutputAssociation_15pg8um">
        <bpmn:targetRef>DOR_Reject</bpmn:targetRef>
      </bpmn:dataOutputAssociation>
    </bpmn:task>
    <bpmn:endEvent id="End_Success" name="流程完成" />
    <bpmn:endEvent id="End_Reject" name="拒绝结束" />
    <bpmn:dataObjectReference id="DOR_Application" name="贷款申请表" dataObjectRef="DO_Application" />
    <bpmn:dataObjectReference id="DOR_Precheck" name="初审意见" dataObjectRef="DO_Precheck" />
