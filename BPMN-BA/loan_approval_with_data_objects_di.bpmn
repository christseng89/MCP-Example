    <bpmn:sequenceFlow id="Flow_4_no_b" sourceRef="Task_NotifyReject" targetRef="End_Reject" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_GenerateContract" targetRef="Task_Disburse" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_Disburse" targetRef="End_Success" />
    <bpmn:dataStoreReference id="DataStoreReference_1ei56eu" name="Loan" />
    <bpmn:dataStoreReference id="DataStoreReference_084wzik" name="Agreement" />
