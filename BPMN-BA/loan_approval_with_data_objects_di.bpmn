    <bpmn:dataObjectReference id="DOR_Reject" name="拒绝通知" dataObjectRef="DO_RejectNotice" />
    <bpmn:sequenceFlow id="Flow_1" sourceRef="Start_Submit" targetRef="Task_Precheck" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_Precheck" targetRef="Task_RiskAssess" />
